	<object-name>DOOR</object-name>
	<condition>		
		<less-than-equals>		
			<property>/sim/time/sun-angle-rad</property>
			<value>1.57</value>
		</less-than-equals>
	</condition>
	<emission>	
		<red>0</red>	
		<green>0</green>	
		<blue>0</blue>
	</emission>
</animation>

