<?xml version="1.0"?>

<PropertyList>

    <path>ENAL_tower.ac</path>

    <animation>
	  <type>range</type>
	  <min-m>0</min-m>
	  <max-m>15000</max-m>
    </animation>

<animation>
	<type>material</type> 
	<object-name>GLASS</object-name>
	<object-name>WINDOW1</object-name>
	<object-name>WINDOW2</object-name>
	<object-name>WINDOW6</object-name>
	<object-name>DOOR</object-name>
	<condition>		
		<greater-than>		
			<property>/sim/time/sun-angle-rad</property>
			<value>1.57</value>
		</greater-than>
	</condition>
	<emission>	
		<red>1</red>	
		<green>0.9</green>	
		<blue>0.8</blue>
	</emission>
</animation>

