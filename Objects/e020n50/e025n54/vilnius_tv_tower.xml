<?xml version="1.0"?>

<PropertyList>

 <path>vilnius_tv_tower.ac</path>
 
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>35000</max-m>
 </animation>
         
<animation>
 <type>material</type> 
 <object-name>Circle</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.6</red>	
  <green>0.6</green>	
  <blue>0.3</blue>
 </emission>
</animation>

</PropertyList>
         
